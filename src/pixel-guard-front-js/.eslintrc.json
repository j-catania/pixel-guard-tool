{
  "extends": "next/core-web-vitals",
  "rules": {
    "linebreak-style": [
      "error",
      "unix"
    ],
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "comma-dangle": [
      2,
      {
        "arrays": "always",
        "objects": "always",
        "imports": "always",
        "exports": "always",
        "functions": "never"
      }
    ]
  }
}
